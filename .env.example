# Firebase Configuration
# Get these values from Firebase Console > Project Settings > Your apps

VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# Gemini API (for urgency scoring - Person 3)
VITE_GEMINI_API_KEY=your_gemini_api_key

# FlowGlad API (for workflow automation - Person 3)
VITE_FLOWGLAD_API_KEY=your_flowglad_api_key

# ElevenLabs (optional voice notifications)
VITE_ELEVENLABS_API_KEY=your_elevenlabs_api_key

# K2 Think (client-side fallback, server recommended)
VITE_K2THINK_API_KEY=your_k2think_api_key

# K2 Think (server-side only - Firebase Functions)
K2_BASE_URL=https://api.k2think.ai/v1/chat/completions
K2_API_KEY=your_k2_api_key
K2_MODEL=MBZUAI-IFM/K2-Think-v2
